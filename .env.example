# Libre Money Webapp Environment Variables
# Copy this file to .env and update the values as needed

# Go Online Registration URL
VITE_GO_ONLINE_REGISTRATION_URL=https://libre.money/#/go-online

# Go Online Self Host URL
VITE_GO_ONLINE_SELF_HOST_URL=https://libre.money/#/self-host

# Demo Preparation Feature Flag
VITE_DEMO_PREPARATION_ENABLED=false

# Dedicated auth server
VITE_AUTH_SERVER_URL=https://auth.libre.money

# Should the audit log be enabled
VITE_AUDIT_LOG_FEATURE_ENABLED=false
